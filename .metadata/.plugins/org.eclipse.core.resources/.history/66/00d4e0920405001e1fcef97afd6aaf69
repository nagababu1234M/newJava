package com.example.CitizenServices_MS;

import jakarta.persistence.Entity;

@Entity
public class Citizen {
 private int id;
 private String name;
 private String vaccinationCenter;
 
public Citizen(int id, String name, String vaccinationCenter) {
	super();
	this.id = id;
	this.name = name;
	this.vaccinationCenter = vaccinationCenter;
}
@Override
public String toString() {
	return "Citizen [id=" + id + ", name=" + name + ", vaccinationCenter=" + vaccinationCenter + "]";
}
public int getId() {
	return id;
}
public void setId(int id) {
	this.id = id;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public String getVaccinationCenter() {
	return vaccinationCenter;
}
public void setVaccinationCenter(String vaccinationCenter) {
	this.vaccinationCenter = vaccinationCenter;
}
}
